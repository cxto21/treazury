{
  "name": "tongo-donation-demo",
  "version": "1.0.0",
  "description": "Private donation demo using Tongo Cash on Starknet",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:web": "vite build",
    "build:deploy": "vite build",
    "dev": "bun run src/demo.ts",
    "dev:web": "vite",
    "serve": "bunx http-server . -p 8080",
    "demo": "bun run build && bun run dist/demo.js",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "deploy": "bun run build:deploy && npx wrangler pages deploy dist --project-name=tongo-ukraine",
    "deploy:login": "npx wrangler login",
    "deploy:build": "bun run build:deploy"
  },
  "keywords": [
    "starknet",
    "tongo",
    "privacy",
    "donations",
    "zk"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@atomiqlabs/chain-starknet": "^7.0.23",
    "@atomiqlabs/sdk": "^7.0.6",
    "@fatsolutions/tongo-sdk": "^1.3.0",
    "circomlibjs": "^0.1.7",
    "dotenv": "^17.2.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "starknet": "^8.9.1",
    "starknetkit": "^2.12.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^24.10.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "3",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  },
  "overrides": {
    "rollup": "^4.28.1"
  }
}
